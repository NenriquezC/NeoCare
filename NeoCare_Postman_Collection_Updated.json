                  "        pm.expect(pm.response.json()).to.have.property('week');",
                  "        pm.expect(pm.response.json()).to.have.property('completed');",
                  "        pm.expect(pm.response.json()).to.have.property('new');",
                  "        pm.expect(pm.response.json()).to.have.property('overdue');",
                  "    });",
                  "    console.log('üìä Semana: ' + pm.response.json().week);",
                  "    console.log('Completadas: ' + pm.response.json().completed.count);",
                  "    console.log('Nuevas: ' + pm.response.json().new.count);",
                  "    console.log('Vencidas: ' + pm.response.json().overdue.count);",
                  "} else {",
